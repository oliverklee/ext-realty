<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<formidable version="1.0.0">
	<meta>
		<name>FE editor form</name>
		<description>This form is for editing realty records in the FE.</description>
		<form>
			<formid>tx_realty_frontEndEditor</formid>
		</form>
		<debug>false</debug>
		<displaylabels>false</displaylabels>
		<minify enabled="1"/>
	</meta>
	<control>
		<datahandler:DB>
			<tablename>tx_realty_objects</tablename>
			<keyname>uid</keyname>
			<process>
				<beforeinsertion>
					<userobj>
						<extension>this</extension>
						<method>modifyDataToInsert</method>
					</userobj>
				</beforeinsertion>
			</process>
		</datahandler:DB>
		<renderer:TEMPLATE>
			<template>
				<path>
					<userobj>
						<php><![CDATA[
							require_once(t3lib_extMgm::extPath('oelib') . 'class.tx_oelib_Autoloader.php');
							return tx_realty_frontEndForm::getTemplatePath();
						]]></php>
					</userobj>
				</path>
				<subpart>###FE_EDITOR###</subpart>
				<errortag>errors</errortag>
			</template>
		</renderer:TEMPLATE>
		<actionlets>
			<actionlet:USEROBJ>
				<userobj>
						<extension>this</extension>
						<method>sendEmailForNewObjectAndClearFrontEndCache</method>
				</userobj>
			</actionlet:USEROBJ>
